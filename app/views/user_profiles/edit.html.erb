<div class='editalign'>
<table>
<%= form_tag :action => 'update' do %>
<%= form_for("user", :as => 'user', :html => { :method => :put }) do |f| %>
<tr>
	<td><%= f.label :email %></td>
  <td><%= f.email_field :email, :autofocus => true,:value=>current_user.email %></td>
</tr>
<tr>
  <td><%= f.label :password %></td>
  <td><%= f.password_field :password, :autocomplete => "off",:value=>current_user.password %></td>
</tr>
<tr>
  <td><%= f.label :password_confirmation %></td>
  <td><%= f.password_field :password_confirmation %></td>
</tr>

<tr>
<td>  <%= f.label :username %></td>
<td><%= f.text_field :username ,:value=>current_user.username%></td>
</tr>
<tr>
<td> 
 <center><button type="submit">Update</button></center></td>
</tr>
<%end%>
<%end%>
</table>
</div>

